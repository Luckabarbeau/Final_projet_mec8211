                X_2(cell_in_swirl)=0;
                V2(cell_in_swirl)=V(cell_in_swirl)+V_specific*m_dot_in*dt;
            end
        else
            
            [P2,T_2(cell_in_swirl),m_vap2,m_dot_out]=swirlpot(m_dot_in,X(cell_in_swirl),P,T(cell_in_swirl),Q_out,m_vap,dt);
            X_2(cell_in_swirl)=0;
