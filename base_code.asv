        V_out=V_out2;
        time=[time t+dt];
        V(V>Volume_total)=V(V>Volume_total)-Volume_total;
        disp('time solved')
        disp(t+dt)
        hold off
        
        %Conduction in water
        
        T=(I/dt-C_M)\(T/dt);
        
        %check degazeur
        select=V>(Volume_tube_1+Volume_m+Volume_swirl+dv);
        check_degaz=find(X(select)~=0);
        if isempty(check_degaz)==1
        else
            disp('degaz_not_respected')
            error('degazage non respecte')
        end
        
%               plot(V(:)',T(:)','.')
